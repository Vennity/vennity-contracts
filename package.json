    "hardhat-gas-reporter": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-node": "^10.0.0",
    "typechain": "^5.1.1",
    "typescript": "^4.3.4",
    "uuid": "^8.3.2"
  }
}