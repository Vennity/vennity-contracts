  "dependencies": {
    "@openzeppelin/contracts": "4.2.0",
    "hardhat": "2.5.0"
  },
  "scripts": {
    "clean": "rimraf ./cache ./artifacts ./types",
    "compile": "npm run clean && npx hardhat compile",
    "compile:optimisticKovan": "npm run clean && npx hardhat compile --network optimisticKovan",
    "test": "npx hardhat test",
    "test:kovan": "npx hardhat test ./test/VennityCollectionKovan.test.ts --network kovan",
    "test:rinkeby": "npx hardhat test ./test/VennityCollectionRinkeby.test.ts --network rinkeby",
    "test:mumbai": "npx hardhat test ./test/VennityCollectionMumbai.test.ts --network mumbai",
    "test:polygonMainnet": "npx hardhat test ./test/VennityCollectionPolygonMainnet.test.ts --network polygonMainnet",
    "test:opKovan": "npx hardhat test ./test/VennityCollectionOptimisticKovan.test.ts --network optimisticKovan"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.1.0",
    "@types/chai": "^4.2.19",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.12.4",
    "@types/uuid": "^8.3.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
